{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowImportingTsExtensions": true,
		"strict": true,
		"noEmit": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"moduleDetection": "force",
		"allowJs": true,
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"types": ["vite/client"]
	},
	"include": ["**/*.ts", "**/*.js", "**/*.tsx", "**/*.jsx"],
	"exclude": ["node_modules", "dist"]
}
